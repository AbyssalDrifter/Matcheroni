  "version": "0.2.0",
  "configurations": [
    {
      "name": "c_parser_test",
      "preLaunchTask": "build_matcheroni",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/c_parser_test",
      "cwd": "${workspaceFolder}",
      //"args" : ["../gcc/gcc"],
      "args" : ["../gcc/gcc/testsuite/gcc.c-torture"],
      //"args" : ["mini_tests"],
      //"args" : ["tests"],
      "setupCommands": [
        {
          // Display content in STL containers pretty
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "benchmark",
      "preLaunchTask": "build_matcheroni",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/benchmark",
      "cwd": "${workspaceFolder}",
      "setupCommands": [
        {
          // Display content in STL containers pretty
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
